{
  "name": "beastbytes/token-db",
  "description": "Token database storage",
  "keywords": [
    "database",
    "token",
    "yii3"
  ],
  "type": "library",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "BSD-3-Clause",
  "require": {
    "ext-pdo": "*",
    "php": "^8.1",
    "beastbytes/token": "dev-master",
    "yiisoft/db": "^1.2",
    "yiisoft/db-migration": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "vimeo/psalm": "^5.0",
    "yiisoft/cache": "^3.0",
    "yiisoft/db-sqlite": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "BeastBytes\\Token\\Db\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BeastBytes\\Token\\Db\\Tests\\": "tests/"
    }
  },
  "suggest": {
    "beastbytes/token-uuid4": "Factory for UUID4 tokens"
  }
}